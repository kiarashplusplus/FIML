    "capabilities": [
        {
            "name": "search-by-symbol",
            "type": "tool",
            "description": "Search for a stock by symbol with instant cached data and async deep analysis",
            "category": "data-query",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "symbol": {
                        "type": "string",
                        "description": "Stock ticker symbol (e.g., TSLA, AAPL)"
                    },
                    "market": {
                        "type": "string",
                        "description": "Market/exchange (US, UK, JP, etc.)",
                        "default": "US"
                    },
                    "depth": {
                        "type": "string",
                        "enum": [
                            "quick",
                            "standard",
                            "deep"
                        ],
                        "description": "Analysis depth",
                        "default": "standard"
                    },
                    "language": {
                        "type": "string",
                        "description": "Response language (en, ja, zh, es, fr, de)",
                        "default": "en"
                    }
                },
                "required": [
                    "symbol"
                ]
            }
        },
        {
            "name": "search-by-coin",
            "type": "tool",
            "description": "Search for cryptocurrency with instant cached data and async deep analysis",
            "category": "data-query",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "symbol": {
                        "type": "string",
                        "description": "Crypto symbol (e.g., BTC, ETH, SOL)"
                    },
                    "exchange": {
                        "type": "string",
                        "description": "Preferred exchange (binance, coinbase, kraken)",
                        "default": "binance"
                    },
                    "pair": {
                        "type": "string",
                        "description": "Trading pair (e.g., USDT, USD, EUR)",
                        "default": "USDT"
                    }
                },
                "required": [
                    "symbol"
                ]
            }
        },
        {
            "name": "get-task-status",
            "type": "tool",
            "description": "Poll or stream updates for an async analysis task",
            "category": "data-query",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "taskId": {
                        "type": "string",
                        "description": "Task ID returned from search-by-symbol or search-by-coin"
                    },
                    "stream": {
                        "type": "boolean",
                        "description": "Stream updates via SSE instead of polling",
                        "default": false
                    }
                },
                "required": [
                    "taskId"
                ]
            }
        },
        {
            "name": "execute-fk-dsl",
            "type": "tool",
            "description": "Execute a Financial Knowledge DSL query for complex multi-step analysis",
            "category": "data-query",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "query": {
                        "type": "string",
                        "description": "FK-DSL query string (e.g., 'EVALUATE TSLA: PRICE, VOLATILITY(30d)')"
                    },
                    "async": {
                        "type": "boolean",
                        "description": "Execute asynchronously and return task ID",
                        "default": true
                    }
                },
                "required": [
                    "query"
                ]
            }
        },
        {
            "name": "create-analysis-session",
            "type": "tool",
            "description": "Create a new analysis session for tracking multi-query workflows",
            "category": "session-management",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "assets": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "List of asset symbols to analyze"
                    },
                    "sessionType": {
                        "type": "string",
                        "enum": [
                            "equity",
                            "crypto",
                            "portfolio",
                            "comparative",
                            "macro"
                        ],
                        "description": "Type of analysis session"
                    }
                },
                "required": [
                    "assets",
                    "sessionType"
                ]
            }
        },
        {
            "name": "get-session-info",
            "type": "tool",
            "description": "Get information about an existing session",
            "category": "session-management",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "sessionId": {
                        "type": "string",
                        "description": "Session UUID"
                    }
                },
                "required": [
                    "sessionId"
                ]
            }
        },
        {
            "name": "list-sessions",
            "type": "tool",
            "description": "List sessions for a user",
            "category": "session-management",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string",
                        "description": "User identifier"
                    },
                    "limit": {
                        "type": "number",
                        "default": 50
                    }
                },
                "required": [
                    "userId"
                ]
            }
        },
        {
            "name": "extend-session",
            "type": "tool",
            "description": "Extend session expiration time",
            "category": "session-management",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "sessionId": {
                        "type": "string",
                        "description": "Session UUID"
                    },
                    "hours": {
                        "type": "number",
                        "default": 24
                    }
                },
                "required": [
                    "sessionId"
                ]
            }
        },
        {
            "name": "get-session-analytics",
            "type": "tool",
            "description": "Get session analytics and statistics",
            "category": "session-management",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string",
                        "description": "Filter by user (optional)"
                    },
                    "days": {
                        "type": "number",
                        "default": 30
                    }
                }
            }
        },
        {
            "name": "warm-cache",
            "type": "tool",
            "description": "Trigger cache warming for specific assets",
            "category": "cache-management",
            "inputSchema": {
                "type": "object",
                "properties": {
                    "symbols": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "List of symbols to warm up"
                    },
                    "concurrency": {
                        "type": "number",
                        "default": 5
                    }
                },
                "required": [
                    "symbols"
                ]
            }
        }
    ],
    "mcp": {
